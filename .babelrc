{
    "presets": [
        "react-native",
        "react",
        ["env", {
            "targets": {
                "browsers": ["last 2 versions", "safari >= 7"]
            }
        }]
    ],
    "plugins": [
        "transform-class-properties",
        "transform-flow-strip-types",
        [
            "react-css-modules", {
                "generateScopedName": "[name]__[local]___[hash:base64:5]"
            }
        ]
    ]
}

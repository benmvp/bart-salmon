{
  "name": "bart-salmon",
  "version": "0.1.0",
  "private": true,
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "flow": "flow",
    "lint": "yarn run lint:js && yarn run lint:css",
    "lint:js": "eslint app",
    "lint:css": "sass-lint --verbose --no-exit",
    "check:static": "yarn run lint",
    "test": "jest --verbose",
    "test:coverage": "yarn test -- --coverage",
    "validate": "yarn run check:static && yarn test",
    "validate:coverage": "yarn run check:static && yarn test:coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "start": "yarn run start:web",
    "start:rn": "react-native start",
    "start:android": "react-native run-android",
    "start:ios": "react-native run-ios",
    "start:web":
      "webpack-dev-server --config web/webpack/web.dev.config.js --inline --hot --colors",
    "bundle:ios":
      "react-native bundle --entry-file index.ios.js --platform ios --bundle-output ios/main.jsbundle",
    "bundle:ios:dev":
      "react-native bundle --dev false --entry-file index.ios.js --platform ios --bundle-output ios/main.jsbundle",
    "bundle:android":
      "react-native bundle --entry-file index.android.js --platform android --bundle-output android/app/src/main/assets/index.android.bundle",
    "bundle:android:dev":
      "react-native bundle --dev false --entry-file index.android.js --platform android --bundle-output android/app/src/main/assets/index.android.bundle",
    "bundle:web":
      "NODE_ENV=production webpack --config web/webpack/web.prod.config.js --progress --colors",
    "deploy:web":
      "yarn run bundle:web && gh-pages --dist web/public --message 'Manual deploy'",
    "gen:routes": "babel-node _scripts/gen-routes-data.js",
    "gen:stations": "babel-node _scripts/gen-stations-data.js",
    "gen:station-routes": "babel-node _scripts/gen-station-routes-data.js",
    "gen:mock:etd": "babel-node _scripts/gen-mock-etd-data.js"
  },
  "jest": {
    "preset": "jest-expo",
    "coveragePathIgnorePatterns": ["/node-modules/", "<rootDir>/app/store/"]
  },
  "engines": {
    "node": ">=6"
  },
  "pre-commit": ["validate"],
  "dependencies": {
    "expo": "^20.0.0",
    "humps": "^2.0.0",
    "lodash": "^4.17.3",
    "react": "^16.0.0-rc.2",
    "react-dom": "^16.0.0-rc.2",
    "react-native": "^0.47.0",
    "react-native-router-flux": "^3.37.0",
    "react-native-web": "^0.0.121",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-persist": "^4.0.1",
    "redux-thunk": "^2.1.0",
    "url-lib": "^2.0.2",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "airbnb-js-shims": "^1.1.1",
    "autoprefixer": "^6.5.4",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.9",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "^1.0.2",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-native": "1.9.0",
    "coveralls": "^2.11.15",
    "css-loader": "^0.26.1",
    "enzyme": "^2.6.0",
    "eslint": "^3.12.1",
    "eslint-config-eventbrite-react": "^2.0.1",
    "eslint-import-resolver-webpack": "^0.8.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.1",
    "eslint-plugin-react": "^6.8.0",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.36.0",
    "gh-pages": "^0.12.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^18.1.0",
    "jest-expo": "~20.0.0",
    "json-loader": "^0.5.4",
    "postcss-loader": "^1.2.1",
    "pre-commit": "^1.2.2",
    "react-hot-loader": "^1.3.1",
    "react-native-scripts": "1.3.1",
    "react-scripts": "^1.0.13",
    "react-test-renderer": "^16.0.0-rc.2",
    "redux-devtools-extension": "^1.0.0",
    "redux-logger": "^2.7.4",
    "resolve-url-loader": "^1.6.1",
    "sass-lint": "^1.10.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "whatwg-fetch": "^2.0.3"
  }
}

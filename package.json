{
	"name": "BartSalmon",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"flow": "flow",
		"lint": "yarn run lint:js && yarn run lint:css",
		"lint:js": "eslint app",
		"lint:css": "sass-lint --verbose --no-exit",
		"check:static": "yarn run lint && yarn run flow",
		"test": "jest --verbose",
		"test:coverage": "yarn test -- --coverage",
		"validate": "yarn run check:static && yarn test",
		"start": "yarn run start:web",
		"start:rn": "node node_modules/react-native/local-cli/cli.js start",
		"start:android": "react-native run-android",
		"start:ios": "react-native run-ios",
		"start:web": "webpack-dev-server --config web/webpack/web.dev.config.js --inline --hot --colors",
		"bundle:ios": "react-native bundle --entry-file index.ios.js --platform ios --bundle-output ios/main.jsbundle",
		"bundle:ios:dev": "react-native bundle --dev false --entry-file index.ios.js --platform ios --bundle-output ios/main.jsbundle",
		"bundle:android": "react-native bundle --entry-file index.android.js --platform android --bundle-output android/app/src/main/assets/index.android.bundle",
		"bundle:android:dev": "react-native bundle --dev false --entry-file index.android.js --platform android --bundle-output android/app/src/main/assets/index.android.bundle",
		"bundle:web": "webpack --config web/webpack/web.prod.config.js --progress --colors",
		"deploy:web": "yarn run bundle:web&&gh-pages -d web/public",
		"gen:routes": "babel-node _scripts/gen-routes-data.js",
		"gen:stations": "babel-node _scripts/gen-stations-data.js",
		"gen:mock:etd": "babel-node _scripts/gen-mock-etd-data.js"
	},
	"dependencies": {
		"babel-plugin-react-css-modules": "^1.1.0",
		"humps": "^2.0.0",
		"isomorphic-fetch": "^2.2.1",
		"lodash": "^4.17.3",
		"react": "^15.4.1",
		"react-dom": "^15.4.1",
		"react-native": "0.39.2",
		"react-redux": "^5.0.1",
		"react-router": "^3.0.0",
		"redux": "^3.6.0",
		"redux-thunk": "^2.1.0",
		"url-lib": "^2.0.2",
		"xml2js": "^0.4.17"
	},
	"devDependencies": {
		"autoprefixer": "^6.5.4",
		"babel-cli": "^6.18.0",
		"babel-core": "^6.20.0",
		"babel-eslint": "^7.1.1",
		"babel-jest": "17.0.2",
		"babel-loader": "^6.2.9",
		"babel-plugin-transform-class-properties": "^6.19.0",
		"babel-plugin-transform-flow-strip-types": "^6.21.0",
		"babel-preset-env": "^1.0.2",
		"babel-preset-react": "^6.16.0",
		"babel-preset-react-native": "1.9.0",
		"css-loader": "^0.26.1",
		"enzyme": "^2.6.0",
		"eslint": "^3.12.1",
		"eslint-config-eventbrite-react": "^2.0.1",
		"eslint-plugin-import": "^2.2.0",
		"eslint-plugin-jsx-a11y": "^3.0.1",
		"eslint-plugin-react": "^6.8.0",
		"flow-bin": "^0.37.4",
		"gh-pages": "^0.12.0",
		"jest": "17.0.3",
		"json-loader": "^0.5.4",
		"node-sass": "^4.0.0",
		"postcss-loader": "^1.2.1",
		"react-hot-loader": "^1.3.1",
		"react-test-renderer": "15.4.1",
		"redux-devtools-extension": "^1.0.0",
		"redux-logger": "^2.7.4",
		"resolve-url-loader": "^1.6.1",
		"sass-lint": "^1.10.2",
		"sass-loader": "^4.0.2",
		"style-loader": "^0.13.1",
		"webpack": "^1.14.0",
		"webpack-dev-server": "^1.16.2"
	},
	"jest": {
		"preset": "react-native"
	}
}

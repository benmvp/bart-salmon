{
    "presets": [
        "react",
        [
            "env", {
                "targets": {
                    "browsers": ["last 2 versions"]
                }
            }
        ]
    ],
    "plugins": [
        "transform-runtime",
        "transform-flow-strip-types",
        [
            "react-css-modules", {
                "generateScopedName": "[name]__[local]___[hash:base64:5]"
            }
        ]
    ]
}
